{
  "name": "administration",
  "version": "2.1.1",
  "private": true,
  "dependencies": {
    "graphql": "^16.6.0",
    "@apollo/client": "^3.6.9",
    "@blueprintjs/core": "^4.10.1",
    "@blueprintjs/datetime": "^4.3.12",
    "@blueprintjs/select": "^4.6.6",
    "@zxing/library": "^0.19.1",
    "core-js": "^3.25.2",
    "date-fns": "^2.29.3",
    "detect-browser": "^5.3.0",
    "fast-text-encoding": "^1.0.6",
    "jspdf": "^2.5.1",
    "long": "^5.2.0",
    "protobufjs": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-move": "^3.0.4",
    "react-router-dom": "6.2.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.5",
    "web-vitals": "^3.0.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.0",
    "@types/blob-stream": "^0.1.30",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-flip-move": "^2.9.12",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "^5.1.26",
    "apollo": "^2.34.0",
    "run-script-os": "^1.1.6",
    "protobufjs-cli": "^1.0.2",
    "typescript": "^4.8.3",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@graphql-codegen/typescript-operations": "2.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "generate-graphql": "graphql-codegen --config graphql-codegen.yml",
    "generate-protobuf": "run-script-os",
    "generate-protobuf:linux": "mkdir -p src/generated && pbjs -t static-module -w es6 -o src/generated/protobuf.js ../specs/card_activate_model.proto --force-long && pbts -o src/generated/protobuf.d.ts src/generated/protobuf.js",
    "generate-protobuf:win32": "mkdir src\\generated | pbjs -t static-module -w es6 -o src\\generated\\protobuf.js ..\\specs\\card_activate_model.proto --force-long | pbts -o src\\generated\\protobuf.d.ts src\\generated\\protobuf.js",
    "postinstall": "npm run generate-protobuf && npm run generate-graphql"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": "src/generated/**"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 11"
    ]
  }
}
