{
  "name": "administration",
  "version": "3.1.2",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@blueprintjs/core": "^5.0.0",
    "@blueprintjs/popover2": "^2.0.0",
    "@blueprintjs/select": "^5.0.0",
    "@blueprintjs/table": "^5.0.0",
    "@bufbuild/protobuf": "^1.2.1",
    "@fontsource/roboto": "^5.0.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@zxing/library": "^0.20.0",
    "apollo-upload-client": "^17.0.0",
    "date-fns": "^2.30.0",
    "graphql": "^16.7.1",
    "localforage": "^1.10.0",
    "notistack": "^3.0.1",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-move": "^3.0.5",
    "react-router-dom": "^6.14.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^1.2.1",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/blob-stream": "^0.1.30",
    "@types/jasmine": "^4.3.4",
    "@types/node": "^20.3.1",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "apollo": "^2.34.0",
    "css-loader": "^6.8.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "jasmine": "^5.0.2",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "set -ae; REACT_APP_APPLICATION_COMMIT=$(./application_commit.sh);react-scripts start",
    "build": "set -ae; REACT_APP_APPLICATION_COMMIT=$(./application_commit.sh);react-scripts build",
    "test": "karma start --single-run",
    "test:watch": "karma start --autowatch",
    "eject": "react-scripts eject",
    "lint": "eslint . && prettier . --check",
    "format": "eslint . --fix && prettier . --write",
    "generate-graphql": "graphql-codegen --config graphql-codegen.yml",
    "generate-protobuf": "protoc -I ../specs --es_out=src/generated --es_opt=target=ts --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es ../specs/card.proto",
    "postinstall": "npm run generate-protobuf && npm run generate-graphql"
  },
  "eslintConfig": {
    "plugins": [
      "jasmine"
    ],
    "extends": [
      "eslint-config-prettier",
      "react-app",
      "plugin:jasmine/recommended"
    ],
    "rules": {
      "jasmine/new-line-before-expect": "off",
      "jasmine/prefer-toHaveBeenCalledWith": "off"
    },
    "ignorePatterns": [
      "src/generated/**",
      "build/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 11"
    ]
  }
}
